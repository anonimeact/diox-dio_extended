{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Generate Freezed & Json Serializable",
      "type": "shell",
      "command": "flutter",
      "args": [
        "pub",
        "run",
        "build_runner",
        "build",
        "--delete-conflicting-outputs"
      ],
      "options": {
        "cwd": "${workspaceFolder}/example"
      },
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "presentation": {
        "reveal": "always",
        "panel": "dedicated",
        "clear": true
      }
    }
  ]
}